<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE file PUBLIC "-//paradise-office//DTD text-processing 1.0//EN" "http://www.linux-paradise.co.uk/development/DTDs/text-processing_1.dtd">
  <dialog name="Convert file" type="modal">
    <field_box>
      <row num="1">
        <label name="DOC_TITLE"></label>
      </row>
      <row num="2">
        <combo_box name="Copy 1" plugin="file_type_list" />
        <combo_box name="Copy 2" plugin="file_type_list" />
      </row>
    </field_box>
      <button name="Convert files" command="ok" /><button name="Cancel" command="cancel" />
  </dialog>
