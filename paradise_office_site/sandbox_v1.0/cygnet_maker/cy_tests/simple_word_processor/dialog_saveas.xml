<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<!DOCTYPE dialog PUBLIC "-//paradise-office//DTD cygnet-dialog 0.1//EN" "http://www.linux-paradise.co.uk/DTDs/cygnet/dialogs.dtd">
  <dialog type="modal" name="Save As">
    <field_box>
      <row num="1">
        <label name="File name: " />
        <text_field name="alt_file_name" command="save_as_dl" />
      </row>
    </field_box>
      
    <button name="OK" command="ok" /><button name="Cancel" command="cancel" />
  </dialog>
  