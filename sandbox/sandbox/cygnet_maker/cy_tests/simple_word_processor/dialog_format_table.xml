<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<!DOCTYPE dialog PUBLIC "-//paradise-office//DTD cygnet-dialog 0.1//EN" "http://www.linux-paradise.co.uk/DTDs/cygnet/dialogs.dtd">
  <dialog type="non-modal" name="Format table">
    <field_box>
      <row num="1">
        <large_icon name="None" pic="table_no_format.png" plugin="format_table"
    options="none" />
        <large_icon name="Table 1" pic="table_1.png" plugin="format_table"
    options="table_1" />
        <large_icon name="Table 2" pic="table_2.png" plugin="format_table"
    options="table_2" />
      </row>
      <row num="2">
        <large_icon name="Table 3" pic="table_3.png" plugin="format_table"
    options="table_3" />
        <large_icon name="Table 4" pic="table_4.png" plugin="format_table"
    options="table_4" />
        <large_icon name="Table 5" pic="table_5.png" plugin="format_table"
    options="table_5" />
      </row>
      <row num="3">
        <large_icon name="Table 6" pic="table_6.png" plugin="format_table"
    options="table_6" />
        <large_icon name="Table 7" pic="table_7.png" plugin="format_table"
    options="table_7" />
        <large_icon name="Table 8" pic="table_8.png" plugin="format_table"
    options="table_8" />
      </row>
    </field_box>
      
  <button name="OK" command="ok" /><button name="Cancel" command="cancel" />
  </dialog>