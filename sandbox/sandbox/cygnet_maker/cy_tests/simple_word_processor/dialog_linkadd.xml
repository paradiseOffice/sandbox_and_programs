<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<!DOCTYPE dialog PUBLIC "-//paradise-office//DTD cygnet-dialog 0.1//EN" "http://www.linux-paradise.co.uk/DTDs/cygnet/dialogs.dtd">
  <dialog type="modal" name="Add link">
    <field_box>
      <row num="1">
        <label name="Web address" />
        <url_field name="URL" spans="100 chars" plugin="add_link" />
      </row>
    </field_box>
      
    <button name="OK" command="ok" /><button name="Cancel" command="cancel" />
  </dialog>