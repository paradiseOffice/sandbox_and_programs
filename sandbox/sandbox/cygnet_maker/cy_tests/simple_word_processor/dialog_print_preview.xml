<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<!DOCTYPE dialog PUBLIC "-//paradise-office//DTD cygnet-dialog 0.1//EN" "http://www.linux-paradise.co.uk/DTDs/cygnet/dialogs.dtd">
  <dialog type="modal" name="Print preview">
    <field_box>
      <row num="1">
        <print_view name="DOC_TITLE" spans="9 rows" />
      </row>
      <row num="2">
      </row>
      <row num="3">
      </row>
      <row num="4">
      </row>
      <row num="5"></row>
      <row num="6"></row>
      <row num="7"></row>
      <row num="8"></row>
      <row num="9"></row>
    
      <row num="10">
        <button name="2 page view" plugin="set_print_view" options="2_page" />
        <button name="Back" plugin="last_page_view" />
        <button name="Next" plugin="next_page_view" />
        <large_icon name="Print" pic="print.png" open_browser="print_box" />
      </row>
    </field_box>
    
    <button name="OK" command="ok" /><button name="Cancel" command="cancel" />
  </dialog>